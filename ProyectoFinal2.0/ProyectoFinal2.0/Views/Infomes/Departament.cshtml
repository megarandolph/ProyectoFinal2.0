@model IEnumerable<ProyectoFinal2._0.Departamentos>

@{
    ViewBag.Title = "Departament";
}

<h2>Departamentos</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.dep_codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombre)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.dep_codigo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nombre)
            </td>

        </tr>
    }

    </table>